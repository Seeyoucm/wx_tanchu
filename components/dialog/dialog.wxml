<!--components/dialog/dialog.wxml-->
<view class="wx_test_container" hidden="{{!isShow}}">
    <view class='wx_mask'></view>
    <view class="wx_test">
      <view class='wx_test_title'>{{title}}</view>
      <view class='wx_test_content'>{{content}}</view>
      <view class='wx_test_footer'>
        <view class='wx_test_btn' catchtap='_cancleEvent'>{{cancelText}}</view>
        <view class='wx_test_btn' catchtap='_confirmEvent'>{{confirmText}}</view>
      </view>
    </view>
</view>
